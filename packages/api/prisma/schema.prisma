datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

// Define datamodels here
model UserExample {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  name  String?
}